{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "EXPO:SECTION": "-------------------  EXPO SECTION -------------------",
    "expo:android": "npm run sys:nvm && expo start --android",
    "expo:ios": "npm run sys:nvm && expo start --ios",
    "expo:web": "npm run sys:nvm && expo start --web",
    "expo:web:ci": "npm run sys:nvm && expo start --web",
    "expo:hybrid": "export development=true && npm run sys:nvm &&  expo start --android --web",
    "expo:install": "npm run sys:nvm && expo install",
    "expo:refresh": "adb shell am force-stop host.exp.exponent",
    "firebase:stage": "firebase use development && firebase deploy",
    "firebase:production": "firebase use production && firebase deploy",
    "firebase:functions": "export development=true && firebase functions:shell",
    "COMPILE:SECTION": "-------------------  COMPILE SECTION -------------------",
    "compile:web": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && expo build:web",
    "compile:android": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && expo build:android",
    "compile:ios": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && export SENTRY_LOG_LEVEL='debug' && expo build:ios && expo upload:ios",
    "DEPLOY:SECTION": "-------------------  DEPLOY SECTION -------------------",
    "upload:ios": "expo upload:ios",
    "deploy:development": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish -c --release-channel development && npm run compile:web",
    "deploy:production": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish && npm run compile:web",
    "SYS:SECTION": "-------------------  SYS SECTION -------------------",
    "sys:bust": "npm cache clean --force && rm -fr $TMPDIR/metro* && watchman watch-del-all && jest --clearCache || npm i -g jest && jest --clearCache || echo 'Failed to bust Jest, try manually'",
    "sys:baseline": "npm run sys:nvm && npm run sys:bust && rm package-lock.json && rm -rf node_modules && npm i",
    "sys:nvm": "unset PREFIX && source ~/.nvm/nvm.sh > /dev/null && nvm install &> /dev/null && nvm use || echo 'No NVM available' > /dev/null ",
    "sys:ncu:dry": "npm run sys:nvm && ncu --target patch && ncu --target minor && ncu",
    "sys:update": "npm run sys:nvm && ncu -u --target minor && npm i",
    "sys:upgrade": "npm run sys:nvm && ncu -u expo-cli && npm i && expo upgrade && npm i",
    "TEST:SECTION": "-------------------  TEST SECTION -------------------",
    "test:all": "npm run sys:nvm && npm run test:e2e && jest --colors --silent snap && jest --colors --silent unit",
    "test:e2e:dev": "npm run sys:nvm && cypress open --config video=false,defaultCommandTimeout=5000,requestTimeout=2000",
    "test:e2e": "npm run sys:nvm && start-server-and-test 'npm run expo:web:ci' http://localhost:19006 'cypress run --quiet' ",
    "test:update": "npm run sys:nvm && jest --colors --updateSnapshot snap"
  },
  "dependencies": {
    "@babel/core": "~7.12.16",
    "@neverdull-agency/expo-unlimited-secure-store": "^1.0.10",
    "babel-preset-expo": "8.3.0",
    "base-64": "^1.0.0",
    "color": "^3.1.3",
    "expo": "^40.0.1",
    "expo-av": "~8.7.0",
    "expo-cli": "^4.1.6",
    "expo-constants": "~9.3.5",
    "expo-contacts": "~8.6.0",
    "expo-file-system": "~9.3.0",
    "expo-firebase-analytics": "~2.6.0",
    "expo-firebase-core": "~1.3.0",
    "expo-image-manipulator": "~8.4.0",
    "expo-image-picker": "~9.2.1",
    "expo-linking": "~2.1.1",
    "expo-network": "~2.4.0",
    "expo-permissions": "~10.0.0",
    "expo-random": "~10.0.0",
    "expo-screen-orientation": "~2.1.0",
    "expo-secure-store": "~9.3.0",
    "expo-updates": "~0.4.1",
    "firebase": "7.24.0",
    "history": "^4.10.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-gesture-handler": "~1.10.0",
    "react-native-paper": "^4.7.1",
    "react-native-svg": "12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-web": "~0.14.13",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-promise-middleware": "^6.1.2",
    "sentry-expo": "^3.0.5",
    "use-interval": "^1.3.0",
    "uuid": "^8.3.2",
    "expo-notifications": "~0.9.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "cypress": "^6.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-universe": "^7.0.1",
    "eslint-plugin-react": "^7.22.0",
    "firebase-tools": "^9.3.0",
    "jest": "^26.6.3",
    "jest-expo": "^40.0.2",
    "jest-svg-transformer": "^1.0.0",
    "ncu": "^0.2.1",
    "npm-check-updates": "^10.3.1",
    "react-native-dotenv": "^2.5.3",
    "react-test-renderer": "^17.0.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "sha3": "^2.1.4",
    "start-server-and-test": "^1.12.0"
  },
  "private": true
}
