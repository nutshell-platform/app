{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "export development=true expo start",
    "android": "export development=true && expo start --android",
    "ios": "export development=true && expo start --ios",
    "web": "export development=true && expo start --web",
    "hybrid": "export development=true && expo start --android --web",
    "test": "jest",
    "refresh": "adb shell am force-stop host.exp.exponent",
    "debug": "adb shell input keyevent 82",
    "stage": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish -c --release-channel staging && npm run compile:web && firebase use staging && firebase deploy",
    "compile:web": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo build:web",
    "compile:android": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo build:android",
    "compile:ios": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo build:ios",
    "publish": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish && npm run compile:web && firebase deploy hosting",
    "bust": "npm cache clean --force && rm -fr $TMPDIR/metro* && watchman watch-del-all",
    "reset": "npm run bust && rm package-lock.json && rm -rf node_modules && npm i",
    "diagnostics": "expo diagnostics"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|redux-persist-expo-securestore)"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@sentry/browser": "^5.15.5",
    "babel-preset-expo": "^8.1.0",
    "expo": "^37.0.7",
    "expo-cli": "^3.18.6",
    "expo-constants": "~9.0.0",
    "expo-image-manipulator": "~8.1.0",
    "expo-image-picker": "~8.1.0",
    "expo-permissions": "~8.1.0",
    "expo-random": "~8.1.0",
    "expo-screen-orientation": "~1.0.0",
    "expo-secure-store": "~8.1.0",
    "history": "^4.10.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-gesture-handler": "~1.6.1",
    "react-native-paper": "^3.8.0",
    "react-native-web": "^0.11.7",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-native": "^5.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "redux-promise-middleware": "^6.1.2",
    "sentry-expo": "^2.0.3",
    "uuid": "^7.0.3",
    "firebase": "7.9.0"
  },
  "devDependencies": {
    "firebase-tools": "^8.1.1",
    "jest": "^25.4.0",
    "jest-expo": "^37.0.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "^16.13.1",
    "redux-logger": "^3.0.6",
    "sha3": "^2.1.2"
  },
  "private": true
}
