{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "export development=true && source ~/.nvm/nvm.sh && nvm use && expo start",
    "android": "export development=true && source ~/.nvm/nvm.sh && nvm use && expo start --android",
    "ios": "export development=true && source ~/.nvm/nvm.sh && nvm use && expo start --ios",
    "web": "export development=true && source ~/.nvm/nvm.sh && nvm use && expo start --web",
    "hybrid": "export development=true && source ~/.nvm/nvm.sh && nvm use && expo start --android --web",
    "test": "./node_modules/jest/bin/jest.js --colors --silent",
    "refresh": "adb shell am force-stop host.exp.exponent",
    "debug": "adb shell input keyevent 82",
    "stage": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish -c --release-channel staging && npm run compile:web",
    "firebase:stage": "firebase use staging && firebase deploy",
    "firebase:production": "firebase use production && firebase deploy",
    "firebase:functions": "export development=true && firebase functions:shell",
    "compile:web": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && expo build:web",
    "compile:android": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && expo build:android",
    "compile:ios": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && export SENTRY_LOG_LEVEL='debug' && expo build:ios && expo upload:ios",
    "upload:ios": "expo upload:ios",
    "publish": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish && npm run compile:web",
    "bust": "npm cache clean --force && rm -fr $TMPDIR/metro* && watchman watch-del-all && jest --clearCache",
    "baseline": "source ~/.nvm/nvm.sh && nvm use && npm run bust && rm package-lock.json && rm -rf node_modules && npm i",
    "diagnostics": "expo diagnostics",
    "i": "source ~/.nvm/nvm.sh && nvm use && expo install",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@babel/core": "^7.12.1",
    "@neverdull-agency/expo-unlimited-secure-store": "^1.0.10",
    "@sentry/browser": "^5.26.0",
    "babel-preset-expo": "^8.3.0",
    "base-64": "^0.1.0",
    "color": "^3.1.3",
    "expo": "^39.0.3",
    "expo-cli": "^3.28.0",
    "expo-constants": "~9.2.0",
    "expo-contacts": "~8.6.0",
    "expo-file-system": "~9.2.0",
    "expo-firebase-analytics": "~2.5.1",
    "expo-firebase-core": "~1.2.0",
    "expo-image-manipulator": "~8.3.0",
    "expo-image-picker": "~9.1.1",
    "expo-linking": "^1.0.4",
    "expo-network": "~2.3.0",
    "expo-permissions": "~9.3.0",
    "expo-random": "~9.0.0",
    "expo-screen-orientation": "~2.0.0",
    "expo-secure-store": "~9.2.0",
    "expo-updates": "~0.3.5",
    "firebase": "7.24.0",
    "history": "^4.10.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.3.tar.gz",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-paper": "^4.2.0",
    "react-native-svg": "12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "~0.13.7",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-promise-middleware": "^6.1.2",
    "sentry-expo": "^3.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "cypress": "^5.5.0",
    "dotenv": "^8.2.0",
    "firebase-tools": "^8.12.1",
    "jest": "^26.5.3",
    "jest-expo": "^39.0.0",
    "jest-svg-transformer": "^1.0.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "^16.14.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "sha3": "^2.1.3"
  },
  "private": true
}
